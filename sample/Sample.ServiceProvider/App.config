<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="serviceGroup">
      <section name="serviceConfig" type="SharpService.Configuration.ServiceConfigurationHandler,  SharpService" />
      <section name="classConfig" type="SharpService.Configuration.ClassConfigurationHandler,  SharpService" />
      <section name="registryConfig" type="SharpService.Configuration.RegistryConfigurationHandler,  SharpService" />
    </sectionGroup>
    <section name="exceptionless" type="Exceptionless.ExceptionlessSection, Exceptionless" />
  </configSections>
  <serviceGroup>
    <serviceConfig>
      <service 
        ref="sampleService" 
        interface="Sample.Service.ISampleService" 
        assembly="Sample.Service" 
        binding="nettcp" 
        security="0" 
        export="sampleService:10001" 
        version="1.0.0" 
        enable="true" />
      <service
       ref="sampleService"
       interface="Sample.Service.ISampleService"
       assembly="Sample.Service"
       binding="nettcp"
       security="0"
       export="sampleService:10002"
       version="1.0.0"
       enable="true" />
    </serviceConfig>
    <classConfig>
      <class id="sampleService" type="Sample.Business.SampleBusiness" assembly="Sample.Business" />
    </classConfig>
    <registryConfig>
      <!--<registry regprotocol="local" name="registrylocal" />-->
      <registry  regprotocol="zookeeper" name="registryzookeeper"  address="127.0.0.1:2181" connectTimeout="2000"/>
      <!--<registry  regprotocol="consul" name="registryconsul"  address="127.0.0.1:2181" connectTimeout="2000"/>-->
    </registryConfig>
  </serviceGroup>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
  </startup>
</configuration>